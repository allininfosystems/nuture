<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
     <body>	
     <referenceBlock name="breadcrumbs">
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">Home</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Home</item>
                    <item name="label" xsi:type="string">Home</item>
                    <item name="link" xsi:type="string">{{baseUrl}}</item>
                </argument>
            </action>
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">FAQs</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">FAQs</item>
                    <item name="label" xsi:type="string">FAQs</item>
                </argument>
            </action>
        </referenceBlock>	
        <referenceContainer name="content">			
			<block class="Bluethink\Faq\Block\Index\Index" name="index_index" template="index/index.phtml">
			</block>
        </referenceContainer>      
    </body>
</page>
